{
  "configurations": [
    {
      "name": "Debug Redis Conductor",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "CONDUCTOR_SECRET": "notreal"
      }
    },
    {
      "name": "Debug Mock Conductor",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "CONDUCTOR_SECRET": "notreal"
      }
    },
    {
      "name": "Debug Mock Conf File Conductor",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "args": ["-config=conductor.conf"],
      "cwd": "${workspaceFolder}",
      "env": {
        "CONDUCTOR_SECRET": "notreal"
      }
